{
  "name": "backend-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "services:build": "docker compose build",
    "services:install": "docker compose run app npm install",
    "services:up": "docker compose up",
    "services:stop": "docker compose stop",
    "services:down": "docker compose down",
    "services:test": "docker compose run app npm run test:w",
    "start": "npm run build && node dist/src/server.js",
    "start:dev": "tsx watch --clear-screen=false --nolazy --inspect=0.0.0.0:9229 src/server.ts",
    "test": "jest --config ./jest.config.js",
    "test:w": "jest --watch --config ./jest.config.js --detectOpenHandles",
    "test:c": "jest --coverage --config ./jest.config.js",
    "test:e2e": "jest --config ./jest-e2e.config.js",
    "prepare": "husky",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write src tests docs migrations",
    "migration:create": "ts-node ./migrations/generate.ts",
    "migration:run": "ts-node ./migrations/run.ts",
    "migration:rollback": "ts-node ./migrations/rollback.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
